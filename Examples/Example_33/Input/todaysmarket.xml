  </SwaptionVolatilities>
  <!-- yield volatility definition -->
  <YieldVolatilities id="default">
    <YieldVolatility name="SECURITY_1">YieldVolatility/EUR/EUR-SECURITY_1</YieldVolatility>
  </YieldVolatilities>
