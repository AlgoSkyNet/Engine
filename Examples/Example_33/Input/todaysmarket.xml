    <SwapIndex name="EUR-CMS-1Y">
      <Discounting>EUR-EONIA</Discounting>
    </SwapIndex>
    <SwapIndex name="EUR-CMS-2Y">
      <Discounting>EUR-EONIA</Discounting>
