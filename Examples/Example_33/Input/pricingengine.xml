<PricingEngines>

  <Product type="Swap">
    <Model>DiscountedCashflows</Model>
    <ModelParameters/>
    <!--<Engine>QUANTLIB</Engine>-->
    <Engine>DiscountingSwapEngine</Engine>
    <EngineParameters/>
  </Product>

  <Product type="CrossCurrencySwap">
    <Model>DiscountedCashflows</Model>
    <ModelParameters/>
    <Engine>DiscountingCrossCurrencySwapEngine</Engine>
    <EngineParameters/>
  </Product>
  
  <Product type="EuropeanSwaption">
    <Model>BlackBachelier</Model>
    <!-- depends on input vol -->
    <ModelParameters/>
    <Engine>BlackBachelierSwaptionEngine</Engine>
    <EngineParameters/>
  </Product>									

  <Product type="BermudanSwaption">
    <Model>LGM</Model>
    <ModelParameters>
      <Parameter name="Calibration">Bootstrap</Parameter>
      <Parameter name="CalibrationStrategy">CoterminalATM</Parameter>
      <Parameter name="Reversion">0.0050</Parameter>
      <Parameter name="ReversionType">HullWhite</Parameter>
      <Parameter name="Volatility">0.01</Parameter>
      <Parameter name="VolatilityType">Hagan</Parameter>
      <Parameter name="Tolerance">0.0001</Parameter>
    </ModelParameters>
    <Engine>Grid</Engine>
    <EngineParameters>
      <Parameter name="sy">3.0</Parameter>
      <Parameter name="ny">10</Parameter>
      <Parameter name="sx">3.0</Parameter>
      <Parameter name="nx">10</Parameter>
    </EngineParameters>
  </Product>

  <Product type="StructuredLoan">
    <Model>CrossAssetModel</Model>
      <!-- Certain parameters can be made currency dependend by adding -->
      <!-- _CCY to the parameter name; if not found for the deal ccy   -->
      <!-- the default setting (without _CCY) will be used. The        -->
      <!-- parameters supporting this are reversion, volatility, and   -->
      <!-- all correlation parameters.                                 -->
    <ModelParameters>
      <!-- ==================== IR ======================= -->
      <Parameter name="IrCalibration">Bootstrap</Parameter>
      <Parameter name="IrCalibrationStrategy">CoterminalATM</Parameter>
      <Parameter name="IrReversion">0.0050</Parameter>
      <!-- increased tolerance for testing due to calibration issues in GBP, AUD -->
      <Parameter name="IrTolerance">0.001</Parameter>
      <!-- IrVolatility will usually be calibrated to market instruments -->
      <Parameter name="IrVolatility">0.01</Parameter>
      <Parameter name="IrReversionType">HullWhite</Parameter>
      <Parameter name="IrVolatilityType">Hagan</Parameter>
      <Parameter name="IrVolatilityType">Hagan</Parameter>
       <!-- for analytic cms spread option pricing -->
      <Parameter name="IrCmsCorrelation">1.0</Parameter>													  
      <!-- ==================== INF ======================= -->
      <Parameter name="InfCalibration">Bootstrap</Parameter>
      <Parameter name="InfCalibrationStrategy">ZeroCouponCapFloor</Parameter>
      <!-- <Parameter name="InfCalibration">None</Parameter> -->
      <!-- <Parameter name="InfCalibrationStrategy">None</Parameter> -->
      <Parameter name="InfCalibrationStrike">0.0</Parameter>
      <Parameter name="InfCalibrationUseCaps">false</Parameter>
      <Parameter name="InfReversion">1.00</Parameter>
      <!-- InfVolatility will usually be calibrated to market instruments -->
      <Parameter name="InfVolatility">0.01</Parameter>
      <!--<Parameter name="InfReversionType">HullWhite</Parameter>-->
      <Parameter name="InfReversionType">Hagan</Parameter>
      <Parameter name="InfVolatilityType">Hagan</Parameter>
      <Parameter name="InfTolerance">0.0010</Parameter>
      <!-- ==================== CR ======================== -->
      <!--SpreadDiscounting (old), DeterministicDefault (new) und DynamicDefault (to be) -->
      <Parameter name="DefaultModel_StructuredLoan">SpreadDiscounting</Parameter>
      <Parameter name="DefaultModel_SubordinatedLoan">DeterministicDefault</Parameter>
      <Parameter name="CrCalibration">None</Parameter>
      <Parameter name="CrCalibrationStrategy">None</Parameter>
      <Parameter name="CrReversion">0.01</Parameter>
      <Parameter name="CrReversionType">HullWhite</Parameter>
      <Parameter name="CrVolatilityType">HullWhite</Parameter>
      <Parameter name="CrTolerance">0.0001</Parameter>
      <!--default currencies-->
      <Parameter name="CrVolatility_EUR">0.00</Parameter>
      <Parameter name="CrVolatility_USD">0.00</Parameter>
      <Parameter name="CrVolatility_GBP">0.00</Parameter>
      <!--LANXESS-->
      <Parameter name="CrVolatility_EUR_BB_Chemicals">0.005</Parameter>
      <!--Kiloutou-->
      <Parameter name="CrVolatility_EUR_BB_EquipmentRental">0.0180</Parameter>
      <!--InfraDebt universe EUR-->
      <Parameter name="CrVolatility_EUR_A3_Roads">252,GENERIC-EUR.UTIL.A.10Y,GENERIC-EUR.SWAP.10Y,10.0</Parameter>
      <Parameter name="CrVolatility_EUR_LNDBAWU_Roads">252,GENERIC-EUR.LNDBAWU.10Y,GENERIC-EUR.SWAP.10Y,10.0</Parameter>
      <Parameter name="CrVolatility_EUR_BBB_Utilities">252,GENERIC-EUR.UTIL.BBB.10Y,GENERIC-EUR.SWAP.10Y,10.0</Parameter>
      <Parameter name="CrVolatility_EUR_BBB_Roads">252,GENERIC-EUR.UTIL.BBB.10Y,GENERIC-EUR.SWAP.10Y,10.0</Parameter>
      <Parameter name="CrVolatility_EUR_BBB_Ports">252,GENERIC-EUR.UTIL.BBB.10Y,GENERIC-EUR.SWAP.10Y,10.0</Parameter>
      <Parameter name="CrVolatility_EUR_BBB_Thoroughfares">252,GENERIC-EUR.UTIL.BBB.10Y,GENERIC-EUR.SWAP.10Y,10.0</Parameter>
      <Parameter name="CrVolatility_EUR_BBB_ConstrctnUnverstyBldng">252,GENERIC-EUR.UTIL.BBB.10Y,GENERIC-EUR.SWAP.10Y,10.0</Parameter>
      <Parameter name="CrVolatility_EUR_FRANCE_Ports">252,GENERIC-EUR.UTIL.A.10Y,GENERIC-EUR.SWAP.10Y,10.0</Parameter>
      <Parameter name="CrVolatility_EUR_FRANCE_Roads">252,GENERIC-EUR.UTIL.A.10Y,GENERIC-EUR.SWAP.10Y,10.0</Parameter>
      <!--should be changed to OAT and swap difference-->
      <Parameter name="CrVolatility_EUR_FRANCE_ConstrctnUnverstyBldng">252,GENERIC-EUR.FRANCEOAT.GOV.10Y,GENERIC-EUR.SWAP.10Y,10.0</Parameter>
      <Parameter name="CrVolatility_EUR_BBB_RealEstate">252,GENERIC-EUR.UTIL.BBB.10Y,GENERIC-EUR.SWAP.10Y,10.0</Parameter>
      <Parameter name="CrVolatility_EUR_BBB_Solar">252,GENERIC-EUR.UTIL.BBB.10Y,GENERIC-EUR.SWAP.10Y,10.0</Parameter>
      <Parameter name="CrVolatility_EUR_BBB_AUTOLeasing">252,GENERIC-EUR.INDUST.BBB.4Y,GENERIC-EUR.SWAP.4Y,4.0</Parameter>
      <Parameter name="CrVolatility_EUR_BBB_Airports">252,GENERIC-EUR.INDUST.BBB.5Y,GENERIC-EUR.SWAP.5Y,5.0</Parameter>
      <Parameter name="CrVolatility_GBP_BBB_Airports_UK">252,GENERIC-EUROPE.CDS.INDUST.BBB.5Y,NONE,5.0</Parameter>
      <!--ARC universe EUR-->
      <Parameter name="CrVolatility_EUR_CDSBB_Water">252,GENERIC-EUR.CDS.UTIL.BB.5Y,NONE,5.0</Parameter>
      <Parameter name="CrVolatility_EUR_CDSBBB_PetroOilStorage">252,GENERIC-EUROPE.CDS.INDUST.BBB.5Y,NONE,5.0</Parameter>
      <Parameter name="CrVolatility_EUR_CDSBB_PetroOilStorage">252,GENERIC-EUROPE.CDS.INDUST.BB.5Y,NONE,5.0</Parameter>
      <Parameter name="CrVolatility_EUR_BB_Water">252,GENERIC-EUR.UTIL.BBB.5Y,GENERIC-EUR.SWAP.5Y,5.0</Parameter>
      <Parameter name="CrVolatility_EUR_BBBm_AirportsEquipmentLogistic_UK">252,GENERIC-EUROPE.CDS.INDUST.BBB.5Y,NONE,5.0</Parameter>
      <Parameter name="CrVolatility_EUR_BBp_Communications_Portugal">252,GENERIC-EUROPE.CDS.TELE.BB.5Y,NONE,5.0</Parameter>
      <!--EPC FCT France universe EUR-->
      <Parameter name="CrVolatility_EUR_Bp_Electronics_France">252,GENERIC-EUROPE.CDS.INDUST.B.5Y,NONE,5.0</Parameter>
      <Parameter name="CrVolatility_EUR_Bp_Autoparts_France">252,GENERIC-EUROPE.CDS.CONSUMGOOD.B.5Y,NONE,5.0</Parameter>
      <Parameter name="CrVolatility_EUR_B_MedicalProducts_France">252,GENERIC-EUROPE.CDS.HEALTH.B.5Y,NONE,5.0</Parameter>
      <Parameter name="CrVolatility_EUR_B_Pharmaceuticals_Germany">252,GENERIC-EUROPE.CDS.HEALTH.B.5Y,NONE,5.0</Parameter>
      <!--Africa investments universe-->
      <Parameter name="CrVolatility_EUR_BBB_EAIF">252,GENERIC-USD.SAFRICA.5Y,GENERIC-USD.SWAP.5Y,5.0</Parameter>
      <Parameter name="CrVolatility_USD_BBB_EAIF">252,GENERIC-USD.SAFRICA.5Y,GENERIC-USD.SWAP.5Y,5.0</Parameter>
      <!--USSPT universe EUR-->
      <!--<Parameter name="CrVolatility_EUR_Baa3_Manufacturing">220,GENERIC-EUR.INDUST.BBB.5Y,GENERIC-EUR.SWAP.5Y,5.0</Parameter>-->
      <Parameter name="CrVolatility_EUR_Baa3_Manufacturing">252,GENERIC-NORTHAM.CDS.INDUSTR.BBB.5Y,NONE,5.0</Parameter>
      <!--InfraDebt universe USD-->
      <Parameter name="CrVolatility_USD_BBB_Roads">252,GENERIC-USD.UTIL.BBB.10Y,GENERIC-USD.SWAP.10Y,10.0</Parameter>
      <Parameter name="CrVolatility_USD_BBB_Wind">252,GENERIC-USD.UTIL.BBB.10Y,GENERIC-USD.SWAP.10Y,10.0</Parameter>
      <Parameter name="CrVolatility_USD_BBB_OilGas">252,GENERIC-USD.UTIL.BBB.10Y,GENERIC-USD.SWAP.10Y,10.0</Parameter>
      <Parameter name="CrVolatility_USD_BBB_GasPipline_Mexico">252,GENERIC-USD.UTIL.BBB.10Y,GENERIC-USD.SWAP.10Y,10.0</Parameter>
      <Parameter name="CrVolatility_USD_A_RealEstate">252,GENERIC-USD.UTIL.A.10Y,GENERIC-USD.SWAP.10Y,10.0</Parameter>
      <Parameter name="CrVolatility_USD_BBB_Utilities">252,GENERIC-USD.UTIL.BBB.10Y,GENERIC-USD.SWAP.10Y,10.0</Parameter>
      <Parameter name="CrVolatility_USD_BBB_Solar">252,GENERIC-USD.UTIL.BBB.10Y,GENERIC-USD.SWAP.10Y,10.0</Parameter>
      <Parameter name="CrVolatility_USD_BBBm_Solar">252,GENERIC-USD.UTIL.BBB.10Y,GENERIC-USD.SWAP.10Y,10.0</Parameter>
      <!--Private Debt universe USD-->
      <Parameter name="CrVolatility_USD_NA_OilGas_US">252,GENERIC-USD.ENERGY.BB.5Y,GENERIC-USD.SWAP.5Y,5.0</Parameter>
      <Parameter name="CrVolatility_USD_1BNA_OilGas">252,GENERIC-USD.ENERGY.B.4Y,GENERIC-USD.SWAP.4Y,4.0</Parameter>
      <Parameter name="CrVolatility_USD_NACCC_OilGas_US">252,GENERIC-NORTHAM.CDS.ENG.CCC.5Y,NONE,5.0</Parameter>
      <Parameter name="CrVolatility_USD_BB_Energy_US">252,GENERIC-USD.ENERGY.BB.5Y,GENERIC-USD.SWAP.5Y,5.0</Parameter>
      <Parameter name="CrVolatility_USD_BBBm_Solar_Uruguay">252,GENERIC-USD.URUGUAY.10Y,GENERIC-USD.SWAP.10Y,10.0</Parameter>
      <!--PIMCO universe USD-->
      <Parameter name="CrVolatility_USD_CDSBBB_AviationFinance">252,GENERIC-EUROPE.CDS.INDUST.BBB.5Y,NONE,5.0</Parameter>
      <!--InfraDebt universe GBP-->
      <Parameter name="CrVolatility_GBP_BBB_Ports">252,GENERIC-GBP.UTIL.BBB.10Y,GENERIC-GBP.SWAP.10Y,10.0</Parameter>
      <Parameter name="CrVolatility_GBP_BBB_RealEstate">252,GENERIC-GBP.UTIL.BBB.10Y,GENERIC-GBP.SWAP.10Y,10.0</Parameter>
      <Parameter name="CrVolatility_GBP_A_Roads">252,GENERIC-GBP.UTIL.A.10Y,GENERIC-GBP.SWAP.10Y,10.0</Parameter>
      <Parameter name="CrVolatility_GBP_BBB_Roads">252,GENERIC-GBP.UTIL.BBB.10Y,GENERIC-GBP.SWAP.10Y,10.0</Parameter>
      <!--Private Debt universe GBP-->
      <Parameter name="CrVolatility_GBP_NA_Bioenergy">252,GENERIC-GBP.UTIL.BBB.5Y,GENERIC-GBP.SWAP.5Y,5.0</Parameter>
      <!-- ================ Correlation =================== -->
      <Parameter name="IrInfCorrelation">0.0</Parameter>
      <Parameter name="IrCrCorrelation">0.0</Parameter>
      <Parameter name="InfCrCorrelation">0.0</Parameter>
      <!-- correlations for deal currency EUR -->
      <Parameter name="IrInfCorrelation_EUR">0.0</Parameter>
      <Parameter name="IrCrCorrelation_EUR">0.0</Parameter>
      <Parameter name="InfCrCorrelation_EUR">0.0</Parameter>
      <!-- correlations for deal currency USD -->
      <Parameter name="IrInfCorrelation_USD">0.0</Parameter>
      <Parameter name="IrCrCorrelation_USD">0.0</Parameter>
      <Parameter name="InfCrCorrelation_USD">0.0</Parameter>
      <!-- correlations for deal currency GBP -->
      <Parameter name="IrInfCorrelation_GBP">0.0</Parameter>
      <Parameter name="IrCrCorrelation_GBP">0.0</Parameter>
      <Parameter name="InfCrCorrelation_GBP">0.0</Parameter>
    </ModelParameters>
    <Engine>MC</Engine>
    <EngineParameters>
      <Parameter name="CalibrationSequenceType">PseudoRandom</Parameter>
      <Parameter name="PricingSequenceType">LowDiscrepancy</Parameter>
      <Parameter name="CalibrationSamples">10000</Parameter>
      <Parameter name="PricingSamples">10000</Parameter>
      <Parameter name="CalibrationSeed">42</Parameter>
      <Parameter name="PricingSeed">17</Parameter>
      <Parameter name="TimeSteps">1</Parameter>
      <Parameter name="CalibrationAntithetic">true</Parameter>
      <Parameter name="PricingAntithetic">false</Parameter>
      <Parameter name="CalibrationBrownianBridge">false</Parameter>
      <Parameter name="PricingBrownianBridge">true</Parameter>
      <Parameter name="PolynomOrder">3</Parameter>
      <Parameter name="PolynomType">Monomial</Parameter>
      <Parameter name="HorizonShift">1.0</Parameter>
      <Parameter name="FastCmsApproximation">Y</Parameter>
      <Parameter name="DefaultTimeRngSeed">42</Parameter>
    </EngineParameters>
  </Product>
  <Product type="EquityForward">
    <Model>DiscountedCashflows</Model>
    <ModelParameters/>
    <Engine>DiscountingEquityForwardEngine</Engine>
    <EngineParameters/>
  </Product>
  <Product type="EquityOption">
    <Model>BlackScholesMerton</Model>
    <ModelParameters/>
    <Engine>AnalyticEuropeanEngine</Engine>
    <EngineParameters/>
  </Product>
  <Product type="XXXCliquetOption">
    <Model>BlackScholesMerton</Model>
    <ModelParameters>
      <Parameter name="CalibrationStrike">ATMF</Parameter>
    </ModelParameters>
    <Engine>MC</Engine>
    <EngineParameters>
      <Parameter name="SequenceType">LowDiscrepancy</Parameter>
      <Parameter name="Samples">100000</Parameter>
      <Parameter name="Seed">42</Parameter>
      <!-- BS process has exact discretisation -->
      <Parameter name="TimeStepsPerYear">1</Parameter>
      <Parameter name="BrownianBridge">true</Parameter>
      <Parameter name="Antithetic">false</Parameter>
    </EngineParameters>
  </Product>
<Product type="CliquetOption">
    <Model>Heston</Model>
    <ModelParameters>
      <Parameter name="TimeDependentParameters">false</Parameter>
      <Parameter name="CalibrationStrikes">0.8ATM,0.9ATM,0.95ATM,0.975ATM,1.0ATM,1.025ATM,1.05ATM,1.1ATM,1.2ATM</Parameter>
      <!-- options with premium less than threshold are excluded -->
      <Parameter name="CalibrationPriceThreshold">0.000001</Parameter>
      <!-- rmse in rel price error -->
      <Parameter name="AcceptTolerance">0.17</Parameter>
      <Parameter name="Tolerance">0.50</Parameter>
      <!-- initial values for calibration -->
      <Parameter name="V0">0.04</Parameter>
      <Parameter name="Kappa">0.10</Parameter>
      <Parameter name="Theta">0.03</Parameter>
      <Parameter name="Sigma">0.3</Parameter>
      <Parameter name="Rho">0.0</Parameter>
      <!-- constant params: BestFit, None -->
      <!-- ptd params     : BestFit, BestFitThenBootstrap, None -->
      <Parameter name="Calibration">BestFit</Parameter>
      <!-- Try additional start values for calibration -->
      <Parameter name="CalibrationRestarts">10</Parameter>
      <Parameter name="RestartsSeed">42</Parameter>
    </ModelParameters>
    <Engine>MC</Engine>
    <EngineParameters>
      <Parameter name="SequenceType">LowDiscrepancy</Parameter>
      <Parameter name="Samples">100000</Parameter>
      <Parameter name="Seed">42</Parameter>
      <!-- QE scheme is applied -->
      <Parameter name="TimeStepsPerYear">12</Parameter>
      <Parameter name="BrownianBridge">true</Parameter>
      <Parameter name="Antithetic">false</Parameter>
    </EngineParameters>
  </Product>
  <Product type="FxOption"> 
     <Model>GarmanKohlhagen</Model> 
     <ModelParameters/> 
     <Engine>AnalyticEuropeanEngine</Engine> 
     <EngineParameters/> 
  </Product>
  <Product type="CPICapFloor">
    <Model>CPICapFloorModel</Model>
    <ModelParameters/>
    <!--<Engine>CPICapFloorEngine</Engine>-->
    <Engine>CPIBlackCapFloorEngine</Engine>
    <EngineParameters/> 
  </Product>
</PricingEngines>
