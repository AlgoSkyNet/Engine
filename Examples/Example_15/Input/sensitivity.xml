	<Conventions>
	  <Convention id="IRS">USD-3M-SWAP-CONVENTIONS</Convention>
	  <Convention id="OIS">USD-OIS-CONVENTIONS</Convention>
	</Conventions>
      </ParConversion>
    </DiscountCurve>
    
