	  <Convention id="IRS">EUR-6M-SWAP-CONVENTIONS</Convention>
	  <Convention id="OIS">EUR-OIS-CONVENTIONS</Convention>
	</Conventions>
      </ParConversion>
    </DiscountCurve>
    
