  <!-- Cross Gamma filter: Pairs of factor sub-strings -->
  <CrossGammaFilter>
    <Pair>DISOUNT/EUR,DISCOUNT/EUR</Pair>
    <Pair>INDEX/EUR,INDEX/EUR</Pair>
    <Pair>DISCOUNT/EUR,INDEX/EUR</Pair>      
  </CrossGammaFilter>
  
